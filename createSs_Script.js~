	$('.crSSTopIcon3').addClass('crSSIconNotActive');
	$('.crSSTopIcon4').addClass('crSSIconClickable');
	$('.crSSTopIcon4').removeClass('crSSIconNotActive');
	stepAuth++;
}); // end click
