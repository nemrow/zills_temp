/***************************************************************************** tiny scroll ************************/
#shatBoard {
	width: 560px;
	margin: auto;
	position:relative;
}
#shatBoard .viewport { width: 540px; height: 210px; overflow: hidden; position: relative; }
#shatBoard .overview { list-style: none; position: absolute; left: 0; top:0 ; padding:0 0 5px 0; margin: 0; }
#shatBoard .scrollbar{ background: transparent url(../images/bg-scrollbar-track-y.png) no-repeat 0 0; position: relative; background-position: 0 0; float: right; width: 15px; z-index:5; border-radius:5px; }
#shatBoard .track { background: transparent url(../images/bg-scrollbar-trackend-y.png) no-repeat 0 100%; height: 100%; width:13px; position: relative; padding: 0 1px; border-radius:5px; }
#shatBoard .thumb { background: transparent url(../images/bg-scrollbar-thumb-y.png) no-repeat 50% 100%; height: 20px; width: 25px; cursor: pointer; overflow: hidden; position: absolute; top: 0; left: -5px; border-radius:5px; }
#shatBoard .thumb .end { background: transparent url(../images/bg-scrollbar-thumb-y.png) no-repeat 50% 0; overflow: hidden; height: 5px; width: 25px; border-radius:5px; }
#shatBoard .disable { display: none; }

