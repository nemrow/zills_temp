/********************************************************************************************* timer numbers */
#countdown{
	position:relative;
	float:left;
	z-index:500;
	top:14px;
	margin:2px 0 0 13px;
}
.timer_numbers {
	font-size:25px;
	font-family:Arial, Helvetica, sans-serif;
	font-weight:bold;
	text-align:left;
	color:#ffffff;
}
#timer_days {
	background:url(../images/countdown1.png) no-repeat;
	float:left;
	width:59px;
	height:40px;
	padding:2px 0 0 4px;
	letter-spacing:15px;
}
#timer_hours {
	background:url(../images/countdown1.png) no-repeat;
	float:left;
	width:59px;
	height:40px;
	padding:2px 0 0 4px;
	letter-spacing:15px;
}
#timer_mins {
	background:url(../images/countdown1.png) no-repeat;
	float:left;
	width:59px;
	height:40px;
	padding:2px 0 0 4px;
	letter-spacing:15px;
}
#timer_seconds {
	background:url(../images/countdown2.png) no-repeat;
	float:left;
	width:59px;
	height:40px;
	padding:2px 0 0 4px;
	letter-spacing:15px;
}
